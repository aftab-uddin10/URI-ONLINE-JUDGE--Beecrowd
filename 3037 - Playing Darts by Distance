#include <stdio.h>
 
int main() {
 int N,i,j,x,d,X,D;
 scanf("%d",&N);
 for(i=0;i<N;i++){
     int A=0,B=0;
     for(j=0;j<3;j++){
        scanf("%d %d",&x,&d);
        A=A+(x*d);
     }
     for(j=0;j<3;j++){
        scanf("%d %d",&X,&D);
        B=B+(X*D);
     }
     if(B>A) printf("MARIA\n");
     else printf("JOAO\n");
 }
 
    return 0;
}
