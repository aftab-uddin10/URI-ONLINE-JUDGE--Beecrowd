#include <stdio.h>
int main(){
   int n,a=1,b=0,x,i;
   scanf("%d",&n);
   int A[n];
   for(i=0;i<n;i++){
     A[i]=a;
    x=a+b;
    b=a;
    a=x;
   }
   for(i=n-1;i>=0;i--){
        if(i==0){
          printf("%d",A[i]);
        }else{
          printf("%d ",A[i]);
        }
   }
   printf("\n");
}
