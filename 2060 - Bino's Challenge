#include <stdio.h>
 
int main() {
int N,A,i,a=0,b=0,c=0,d=0;
scanf("%d",&N);
for(i=0;i<N;i++){
scanf("%d",&A);
    if(A%2==0) a++;
    if(A%3==0) b++;
    if(A%4==0) c++;
    if(A%5==0) d++;
    
}
printf("%d Multiplo(s) de 2\n",a);
printf("%d Multiplo(s) de 3\n",b);
printf("%d Multiplo(s) de 4\n",c);
printf("%d Multiplo(s) de 5\n",d);
 
    return 0;
}
