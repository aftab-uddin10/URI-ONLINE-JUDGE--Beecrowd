#include <stdio.h>
 
int main() {
 int a,b,c,d,e,w,s;
 scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);{
     s=b+c;
     if((s%2==0 && a==1) || (s%2==1 && a==0)) w = 1;
    else w = 2;
    if((d==1 && e==0) || (d==0 && e==1)) w = 1;
    else if(d==1 && e==1) w=2;
    printf("Jogador %d ganha!\n", w);
 }
 
 
    return 0;
}
