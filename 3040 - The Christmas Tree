#include <stdio.h>
 
int main() {
 int N,i,a,b,c;
 scanf("%d",&N);
 for(i=0;i<N;i++){
     scanf("%d %d %d",&a,&b,&c);
     if(a >= 200 && a <= 300) {
         if(b >=50){
             if(c >= 150){
             printf("Sim\n");
                 continue ;
             }
         }
     }
     printf("Nao\n");
 }
 
    return 0;
}
